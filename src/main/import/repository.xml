<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <consent-manager j:dependencies="codemirror-editor default"
                       j:modulePriority="0"
                       j:moduleType="module"
                       j:title="Consent Manager"
                       jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder">
            <consents j:contributeTypes="jnt:consentType"
                      j:editableInContribution="true"
                      jcr:mixinTypes="jmix:contributeMode"
                      jcr:primaryType="jnt:contentFolder">
               <j:translation_en jcr:language="en"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="consents"/>
               <j:translation_de jcr:language="de"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="consents"/>
               <j:translation_pt jcr:language="pt"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="consents"/>
               <j:translation_it jcr:language="it"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="consents"/>
               <j:translation_fr jcr:language="fr"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="consentements"/>
               <j:translation_es jcr:language="es"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="consents"/>
               <j:translation_de_DE jcr:language="de_DE"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="consents"/>
            </consents>
         </contents>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
         </templates>
      </consent-manager>
   </modules>
  <sites jcr:primaryType="jnt:virtualsitesFolder">
      <systemsite jcr:primaryType="jnt:virtualsite">
         <categories jcr:primaryType="jnt:category">
            <consent-manager jcr:primaryType="jnt:category">
               <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Consent Manager"/>
               <marketing jcr:primaryType="jnt:category">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Marketing"/>
               </marketing>
               <advertising jcr:primaryType="jnt:category">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Advertising"/>
               </advertising>
               <mandatory jcr:primaryType="jnt:category">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Mandatory"/>
               </mandatory>
            </consent-manager>
         </categories>
      </systemsite>
   </sites>
</content>